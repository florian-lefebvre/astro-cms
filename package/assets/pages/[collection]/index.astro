---
const collectionId = Astro.params.collection

const { resolvedConfig } = Astro.locals.__cms

if (!collectionId || !resolvedConfig.collections.map(e => e.id).includes(collectionId)) {
    return new Response(null, { status: 404, statusText: "Couldn't find collection" })
}

export const prerender = false;
---
<h1>Collection {collectionId}</h1>